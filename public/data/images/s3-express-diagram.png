<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#1a1a1a"/>
  
  <!-- Title -->
  <text x="400" y="60" font-family="Arial, sans-serif" font-size="28" fill="#8B5CF6" text-anchor="middle" font-weight="bold">
    Amazon S3 Express One Zone
  </text>
  
  <!-- Architecture boxes -->
  <rect x="100" y="120" width="200" height="100" fill="#2d2d2d" stroke="#8B5CF6" stroke-width="2" rx="8"/>
  <text x="200" y="165" font-family="Arial, sans-serif" font-size="18" fill="#FFFFFF" text-anchor="middle" font-weight="bold">
    Application
  </text>
  <text x="200" y="190" font-family="Arial, sans-serif" font-size="14" fill="#9CA3AF" text-anchor="middle">
    Your workload
  </text>
  
  <!-- Arrow -->
  <line x1="300" y1="170" x2="480" y2="170" stroke="#3B82F6" stroke-width="3" marker-end="url(#arrowhead)"/>
  <text x="390" y="160" font-family="Arial, sans-serif" font-size="14" fill="#3B82F6" text-anchor="middle">
    Single-digit ms
  </text>
  
  <!-- S3 Express -->
  <rect x="500" y="120" width="200" height="100" fill="#2d2d2d" stroke="#F59E0B" stroke-width="2" rx="8"/>
  <text x="600" y="165" font-family="Arial, sans-serif" font-size="18" fill="#FFFFFF" text-anchor="middle" font-weight="bold">
    S3 Express One Zone
  </text>
  <text x="600" y="190" font-family="Arial, sans-serif" font-size="14" fill="#9CA3AF" text-anchor="middle">
    High-performance storage
  </text>
  
  <!-- Features -->
  <rect x="150" y="280" width="500" height="250" fill="#2d2d2d" stroke="#6B7280" stroke-width="1" rx="8"/>
  <text x="400" y="315" font-family="Arial, sans-serif" font-size="20" fill="#8B5CF6" text-anchor="middle" font-weight="bold">
    Key Features
  </text>
  
  <text x="180" y="355" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF">
    • 10x faster than S3 Standard
  </text>
  <text x="180" y="390" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF">
    • Single-digit millisecond latency
  </text>
  <text x="180" y="425" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF">
    • Optimized for frequently accessed data
  </text>
  <text x="180" y="460" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF">
    • Perfect for ML training &amp; analytics
  </text>
  <text x="180" y="495" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF">
    • Single Availability Zone deployment
  </text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="10" refX="9" refY="3" orient="auto">
      <polygon points="0 0, 10 3, 0 6" fill="#3B82F6"/>
    </marker>
  </defs>
</svg>
